{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/favie/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "tsConfigPath": "apps/favie/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/favie",
  "projects": {
    "favie": {
      "type": "application",
      "root": "apps/favie",
      "entryFile": "main",
      "sourceRoot": "apps/favie/src",
      "compilerOptions": {
        "tsConfigPath": "apps/favie/tsconfig.app.json"
      }
    },
    "auth-service": {
      "type": "application",
      "root": "apps/auth-service",
      "entryFile": "main",
      "sourceRoot": "apps/auth-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth-service/tsconfig.app.json"
      }
    },
    "user-profile-service": {
      "type": "application",
      "root": "apps/user-profile-service",
      "entryFile": "main",
      "sourceRoot": "apps/user-profile-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/user-profile-service/tsconfig.app.json"
      }
    },
    "catalog-service": {
      "type": "application",
      "root": "apps/catalog-service",
      "entryFile": "main",
      "sourceRoot": "apps/catalog-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/catalog-service/tsconfig.app.json"
      }
    },
    "content-delivery-service": {
      "type": "application",
      "root": "apps/content-delivery-service",
      "entryFile": "main",
      "sourceRoot": "apps/content-delivery-service/src",
      "compilerOptions": {
        "tsConfigPath": "apps/content-delivery-service/tsconfig.app.json"
      }
    },
    "entity": {
      "type": "library",
      "root": "libs/entity",
      "entryFile": "index",
      "sourceRoot": "libs/entity/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entity/tsconfig.lib.json"
      }
    },
    "entito": {
      "type": "library",
      "root": "libs/entito",
      "entryFile": "index",
      "sourceRoot": "libs/entito/src",
      "compilerOptions": {
        "tsConfigPath": "libs/entito/tsconfig.lib.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "utils": {
      "type": "library",
      "root": "libs/utils",
      "entryFile": "index",
      "sourceRoot": "libs/utils/src",
      "compilerOptions": {
        "tsConfigPath": "libs/utils/tsconfig.lib.json"
      }
    }
  }
}